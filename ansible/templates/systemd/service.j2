[Unit]
Description={{ item.description }}
After=network.target

[Service]
Type={{ item.type }}
ExecStart={{ item.execstart }}
{%if item.restart is defined %}
Restart={{ item.restart }}
{% endif %}
WorkingDirectory=/opt/minecraft

{%if item.user is defined %}
User={{ item.user }}
Group={{ item.user }}
{% endif %}

[Install]
WantedBy=multi-user.target
